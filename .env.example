# OpenClaw Environment Variables
# Copy this to .env and fill in your API keys


# Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Custom OpenAI-compatible provider (used in openclaw.json models.providers)
MYPROVIDER_BASE_URL=your_myprovider_base_url_here
MYPROVIDER_API_KEY=your_myprovider_api_key_here
# Brave Search API Key (for web_search tool â€” free tier at https://brave.com/search/api/)
BRAVE_API_KEY=your_brave_api_key_here

# Telegram Chat ID for cron news delivery (get via @userinfobot)
TELEGRAM_CHAT_ID=

# Gateway settings
OPENCLAW_PORT=3000
NODE_ENV=production

